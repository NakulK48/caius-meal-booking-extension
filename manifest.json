{
	"manifest_version": 2,

	"name": "Caius",
	"version": "0.0.1",
	"content_scripts": [
		{
			"matches": ["https://www.mealbookings.cai.cam.ac.uk/*"],
			"js": [
				"jquery-1.10.2.js",
				"underscore.js",
				"main.js"
			],
			"css": ["style.css"],
			"run_at" : "document_start"
		}
	],
	"web_accessible_resources": [
		"images/menu-default.png",
		"images/menu-hover.png",
		"images/breadcrumb.png",
		"images/wmd-buttons.png"
	]
}