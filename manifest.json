{
	"manifest_version": 2,

	"name": "Caius Meal booking helper",
	"version": "0.0.15",
	"content_scripts": [
		{
			"matches": ["https://www.mealbookings.cai.cam.ac.uk/*"],
			"js": [
				"jquery-1.10.2.js",
				"jquery.tmpl.min.js",
				"sugar.js",
				"utils.js",
				"HallSummary.js",
				"main.js"
			],
			"css": ["style.css"],
			"run_at" : "document_start"
		}
	],
	"permissions": [
		"storage",
		"https://www.mealbookings.cai.cam.ac.uk/"
	],
	"background": {"page": "background.html"}
}